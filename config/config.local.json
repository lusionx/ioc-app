{
    "env": "local",
    "domain": "test.com",
    "port": 7001,
    "cemRoot": "http://cem.com",
    "kue": {
        "prefix": "q",
        "redis": "redis://127.0.0.1:6379/0?prefix=c2m-",
        "promotion": {
            "interval": 1000
        }
    },
    "odata": {
        "soc": "http://od.nb.besth5.com/soc"
    },
    "logger": {
        "appenders": {
            "app": {
                "type": "file",
                "filename": "logs/app.log"
            },
            "ctx": {
                "type": "file",
                "filename": "logs/ctx.log"
            },
            "console": {
                "type": "console"
            }
        },
        "categories": {
            "default": {
                "appenders": [
                    "app",
                    "console"
                ],
                "level": "debug"
            },
            "ctx": {
                "appenders": [
                    "ctx",
                    "console"
                ],
                "level": "debug"
            }
        }
    },
    "dbConn": "mysql://root:123123@127.0.0.1:3306/ioc-ap"
}

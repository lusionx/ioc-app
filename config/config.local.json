{
    "domain": "test.com",
    "port": 7001,
    "cemRoot": "http://cem.com",
    "kue": {
        "prefix": "-q",
        "redis": "redis://127.0.0.1:6379/0?prefix=c2m",
        "promotion": {
            "interval": 1000
        }
    },
    "logger": {
        "appenders": {
            "app": {
                "type": "file",
                "filename": "logs/app.log"
            },
            "ctx": {
                "type": "file",
                "filename": "logs/ctx.log"
            },
            "console": {
                "type": "console"
            }
        },
        "categories": {
            "default": {
                "appenders": [
                    "app",
                    "console"
                ],
                "level": "debug"
            },
            "ctx": {
                "appenders": [
                    "ctx",
                    "console"
                ],
                "level": "debug"
            }
        }
    }
}
